{
    "manifest_version": 3,
    "name": "Startup Funding Extension",
    "version": "1.0.0",
    "action" : {
      "default_popup": "popup-local.html"
  },
  "options_page": "options.html",
  "description": "Get funding information for startups with one click",
  "icons": {
    "16": "public/16.png",
    "32": "public/32.png",
    "48": "public/48.png",
    "192": "public/192.png"
  },
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "permissions": [
    "activeTab",
    "storage"
  ],
  "host_permissions": [
    "https://funding-proxy.vercel.app/*"
  ]
  }
